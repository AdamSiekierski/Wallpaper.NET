<Window x:Class="Wallpaper.NET.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wallpaper.NET"
        mc:Ignorable="d"
        Title="Settings" Height="350" Width="250">
    <Grid>
        <Label x:Name="label" Content="Settings" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Image x:Name="image" HorizontalAlignment="Center" Margin="0,50,0,0" VerticalAlignment="Top" Width="96" Height="54"/>


    </Grid>
</Window>
